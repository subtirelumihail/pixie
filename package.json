{
  "name": "pixie",
  "version": "0.0.1",
  "description": "A react & redux boilerplate project",
  "main": "src/main.js",
  "author": "Subtirelu Mihail",
  "license": "MIT",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --config configs/webpack/webpack.config.js --env=dev",
    "start:watch:dev": "node_modules/.bin/nodemon --watch configs --exec yarn run start",
    "clean": "rimraf build",
    "build:dev": "yarn run clean && webpack --env=dev --progress --colors --config configs/webpack/webpack.config.js",
    "build:prod": "yarn run clean && webpack --env=prod --progress --colors --config configs/webpack/webpack.config.js",
    "build:test": "yarn run build:prod && cd build && python -m SimpleHTTPServer 8080"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
